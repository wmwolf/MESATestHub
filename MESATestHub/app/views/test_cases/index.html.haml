        %td= test_case.datum_5_type
        %td= test_case.datum_6_name
        %td= test_case.datum_6_type
        %td= test_case.datum_7_name
        %td= test_case.datum_7_type
        %td= test_case.datum_8_name
        %td= test_case.datum_8_type
        %td= test_case.datum_9_name
        %td= test_case.datum_9_type
        %td= test_case.datum_10_name
        %td= test_case.datum_10_type
        %td= link_to 'Show', test_case
        %td= link_to 'Edit', edit_test_case_path(test_case)
        %td= link_to 'Destroy', test_case, method: :delete, data: { confirm: 'Are you sure?' }

