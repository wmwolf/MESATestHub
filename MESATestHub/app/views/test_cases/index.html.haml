      %th Datum 9 type
      %th Datum 10 name
      %th Datum 10 type
      %th
      %th
      %th

  %tbody
    - @test_cases.each do |test_case|
      %tr
        %td= test_case.name
        %td= test_case.version_added
        %td= test_case.description
        %td= test_case.last_version_status
        %td= test_case.last_test_status
